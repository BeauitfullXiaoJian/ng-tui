{
    "name": "example",
    "version": "0.0.0",
    "scripts": {
        "serve": "npx ng serve --host 0.0.0.0 --aot",
        "prod": "npx ng serve --host 0.0.0.0 --prod",
        "build": "ng build --base-href /ng/ --deploy-url https://www.cool1024.com/ng/ --prod --build-optimizer --source-map",
        "build-map": "ng build --prod --source-map",
        "serve-proxy": "npx ng serve --host 0.0.0.0 --aot --proxy-config proxy.json",
        "view": "npx ng build --prod --stats-json && npx webpack-bundle-analyzer@latest ./dist/example/stats.json"
    },
    "private": true,
