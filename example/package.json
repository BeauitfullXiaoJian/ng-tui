{
  "name": "example",
  "version": "0.0.0",
  "scripts": {
    "serve": "npm run dev",
    "dev": "npx ng serve --aot --host 0.0.0.0",
    "dev-en": "npx ng serve --aot --host 0.0.0.0 --configuration=en-US",
    "prod": "npx ng serve --host 0.0.0.0 --prod",
    "build": "npx ng build --base-href /ng/ --deploy-url //www.cool1024.com/ng/ --prod --build-optimizer --source-map",
    "serve-proxy": "npx ng serve --host 0.0.0.0 --aot --proxy-config proxy.json",
    "view": "npx ng build --prod --stats-json && npx webpack-bundle-analyzer@latest ./dist/example/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "angular-sortablejs": "^2.6.0",
    "animate.css": "^3.7.0",
    "bootstrap": "4.1.2",
    "core-js": "^2.5.4",
    "install": "^0.12.2",
    "ng-tui": "0.0.15",
    "rxjs": "~6.3.3",
    "sortablejs": "^1.7.0",
    "xlsx": "^0.14.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
